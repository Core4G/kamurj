{
  "kind": "collectionType",
  "collectionName": "loans",
  "info": {
    "singularName": "loan",
    "pluralName": "loans",
    "displayName": "Loan"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "isTopLoan": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": false
    },
    "widgetImageSrc": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "secured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": false
    },
    "purpose": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::purpose.purpose"
    },
    "iconSrc": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mainImageSrc": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "loan_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::loan-group.loan-group",
      "inversedBy": "loans"
    },
    "showOnMainPage": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "terms": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "details-panel.details-panel",
        "single-row-list.single-row-list",
        "single-row.single-row"
      ]
    },
    "docs": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "single-row.single-row"
      ]
    },
    "loan_currencies": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "loan-currency.loan-currency"
      ]
    }
  }
}
